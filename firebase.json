{
    "hosting": {
        "public": "build",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [{
            "source" : "/bundle.js",
            "headers" : [{
                "key" : "Cache-Control",
                "value" : "no-cache"
            }]
        }]
    }
}